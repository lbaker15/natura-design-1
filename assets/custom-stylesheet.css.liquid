}
.about .left {
    width: 50%; height: 100%;

}
.about .left div {
    width: 100%; height: 100%;
    background-size: cover; background-position: 50% 68%;
}
.about .right {
    width: 50%; height: 100%;
}
.about .right .text-inner {
    width: 85%; margin-left: auto; margin-right: auto;
    margin-top: 100px;
}
.about .right h1 {
    font-family: 'saff';
    font-weight: normal;
    font-size: 35px;
    margin-bottom: 40px;
    color: var(--beigeTextDark);
}
.about .right h3  {
    font-family: 'Manrope';
    text-transform: uppercase;
    font-weight: 300;
    font-size: 28px; line-height: 42px;
    color: var(--beigeTextDark);
}
.about .right h3 {}
.about .right .circle {
    width: 110px; height: 110px; background: var(--color2);
    position:absolute;
    border-radius: 50%;
    margin-left: -55px;
    margin-top: 30px;
}
@keyframes pulse {
    0% {transform: scale(1)}
    60% {transform: scale(1.1)}
    100% {transform: scale(1)}
}
@keyframes rotate {
    0% {transform: rotate(0deg)}
    100% {transform: rotate(360deg)}
}
.fa-arrow-right {
    position: absolute;
    margin-left: 42px;
    margin-top: 41px;
    font-size: 30px;
    color: rgb(247, 240, 240);
}
.about .right .circle:hover:before  {
    animation: pulse 1s linear infinite;
}
.about .right .circle:hover .svg-edit {
    animation: rotate 4s linear infinite;
} 
.about .right .circle::before {
    content: ""; position: absolute; top: -15px; left: -15px;
    background: var(--color2); width: 140px; height: 140px;
    border-radius: 50%;
    opacity: 0.4;
}
.svg-edit {
    width: 90%;
    margin-left: 5%;
    margin-top: 5px;
    fill: var(--beigeTextBrown);
    pointer-events: none;
}
@media (max-width: 1170px) {
    .about {
        height: 950px;
    }
}

@media (max-width: 1000px) {
    .hero-section  h1 {
