/*HEADER*/
.header-custom {
    width: 100%; background: var(--color);
    height: 70px; position: fixed; top: 50px; left: 0px;
    z-index: 999999999; display: flex; justify-content: space-between;
}
.top-bar {
    top: 0; position: fixed; left: 0;
    width: 100%;
    height: 50px;
    z-index: 99999999999;
    background: var(--beigeTextBrown);
}
/* ENDHEADER */

/* CART */
.cart-mine {
    width: 70px;
    display: flex;
    align-items: center;
    cursor: pointer;
    user-select: none;
}
.dropdown-menu-mine {
    background: white;
    margin-top: 28px;
    padding: 20px 0px;
    transform: none !important;
    position:fixed !important; top: 92px !important; 
    right: 0px !important;
    width: auto;
}
.dropdown-mine-mobile {
    display: none;
} 
@media (max-width: 640px) {
    .show-mine { 
        display: none; 
    } 
    .dropdown-mine-mobile {
        display: flex;
    }
}
.total {
    font-size: 16px; font-weight: 500;
    color: var(--beigeTextDark);
}
.price {
    color: var(--color2);
    font-size: 16px; font-weight: 400;
}
.cart-inner .text a {
    font-size: 16px; font-weight: 600;
    margin-bottom: 5px;
}
.cart-inner .text small {
    font-size: 100%;
    margin-bottom: 5px;
}
.cart-inner .text span {
    font-size: 15px;
    color: var(--color2);
    font-weight: 500;
    font-size: 16px;

}
.cart-inner {
    padding: 0px 20px;
    display: grid; 
    grid-template-columns: 100px 220px 50px;
    gap: 10px;
}
.CTA {
    gap: 20px;
}
.text {
    display: flex; flex-direction: column;
    color: var(--beigeTextBrown) !important;
    font-family: Manrope;
}
.text a {
